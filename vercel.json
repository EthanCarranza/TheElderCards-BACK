{
  "version": 2,
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/v1/(.*)",
      "destination": "/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
